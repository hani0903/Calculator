<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="numberKeypad">
        <item name="android:onClick">numberClicked</item>
        <item name="android:textSize">40sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="backgroundTint">@color/keypad</item>
    </style>

    <!-- numberKeypad가 가지고 있는 속성 상속받기 -->
    <style name="operatorKeypad" parent="numberKeypad">
        <item name="backgroundTint">@color/operator</item>
    </style>
</resources>